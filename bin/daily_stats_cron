#!/bin/bash

if [[ ! -d $STATS_DIR ]]; then
	echo "STATS_DIR='$STATS_DIR' is not a directory"
	exit 1
fi

cd $STATS_DIR
git pull

cd $STATS_DIR/../bin
#./dnet

if [[ -z "$(git status --porcelain)" ]];
then
    echo "IT IS CLEAN"
else
    echo "PLEASE COMMIT YOUR CHANGE FIRST!!!"
    echo git status
fi
